<div class="block-feedback">
  <div class="block-feedback__question">
    <span>{{question-text}}</span>
    {{#each options}}
      {{#if answer}}
      <a href="{{answer.href}}" class="block-feedback__answer" title="{{answer.title}}">{{answer.text}}</a>
      {{/if}}
    {{/each}}
  </div>
</div>
